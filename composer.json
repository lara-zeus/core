{
    "name": "lara-zeus/core",
    "description": "the core for zeus packages",
    "keywords": [
        "laravel",
        "lara-zeus",
        "form",
        "core",
        "contact"
    ],
    "homepage": "https://github.com/lara-zeus",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ashraf Monshi",
            "email": "wh7r.com@gmail.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.0",
        "archtechx/laravel-seo": "^0.5.1",
        "calebporzio/sushi": "^2.4",
        "codeat3/blade-iconpark": "^1.4.1",
        "filament/filament": "^2.16.45",
        "filament/notifications": "^2.16.45",
        "illuminate/support": "^8.0 || ^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0 || ^7.11",
        "laravel/pint": "^1.2",
        "nunomaduro/larastan": "^2.2.9",
        "pestphp/pest": "^1.22.2",
        "pestphp/pest-plugin-laravel": "^1.3",
        "pestphp/pest-plugin-livewire": "^1.0",
        "pestphp/pest-plugin-parallel": "^0.3.1",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.2.2",
        "phpunit/phpunit": "^9.5.26"
    },
    "autoload": {
        "psr-4": {
            "LaraZeus\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraZeus\\Core\\Tests\\": "tests"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "test:pest": "vendor/bin/pest --parallel",
        "test:phpstan": "vendor/bin/phpstan analyse",
        "test": [
            "@test:pest",
            "@test:phpstan"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraZeus\\Core\\CoreServiceProvider"
            ],
            "aliases": {
                "Core": "LaraZeus\\Core\\CoreFacade"
            }
        }
    }
}
